;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.rpi.local. root.rpi.local. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; NS records
	IN	NS	ns1.rpi.local.
	IN	NS	ns1.rpi.local.

; Name Server - A records
ns1.rpi.local		      IN	A	10.98.99.1
raspberrypi.rpi.local.        IN      A      10.98.99.1
client.rpi.local.             IN      A      10.98.99.2

; other A records
